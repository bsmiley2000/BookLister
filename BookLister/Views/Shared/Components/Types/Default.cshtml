@{
}

@model IEnumerable<string>

<div class="d-grid gap-2">
    <a asp-controller="Home" asp-action="Index" asp-route-bookType="" asp-route-pageNum ="1" class="btn btn-outline-primary" >Home</a>


    @foreach (string t in Model)
    {
        <a asp-controller="Home" asp-action="Index" asp-route-BookType="@t"  asp-route-pageNum ="1" class="btn @(ViewBag.SelectedType == t ? "btn-primary" : "btn-outline-secondary")">@t</a>
    }
</div>